<configuration>
  
  
  <!--在下面修改监听端口的配置-->
  <appSettings>
    <add key="IP" value="127.0.0.1"/>
    <!--监听IP的配置-->
    <add key="Port" value="1088"/>
    <!--监听端口的配置-->
  </appSettings>
  <connectionStrings>
    <add name="connStr" connectionString="server=.; database=ZHYL; uid=sa; pwd=123456"/>
    <!--数据库的配置-->
  </connectionStrings>
  <!--在上面进行项目的相关配置-->
  
  
  
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>

</configuration>
